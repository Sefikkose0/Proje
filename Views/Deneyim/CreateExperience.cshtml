@using webprojesi.DAL.Entities
@model Deneyim
@{
    ViewData["Title"] = "CreateExperience";
    Layout = "~/Views/Layout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Deneyimler</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Deneyim Ekleme Sayfası</div>
					</div>
					<div class="card-body">
						
					     <form method="post">
							 <label> Firma Bilgisi</label>
							 <input type="text" asp-for="ListeBasi" class="form-control"/>
							 <br />
							<label>Çalışılan Pozisyon</label>
							<input type="text" asp-for="Baslik" class="form-control" />
							<br />
							<label>Tarih Aralığı</label>
							<input type="text" asp-for="Tarih" class="form-control" />
							<br />
							<label>Açıklama</label>
							<input type="text" asp-for="Aciklama" class="form-control" />
							<br />
							<button class="btn btn-primary">Deneyimi Ekle</button>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

